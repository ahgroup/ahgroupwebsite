  language               = {eng},
  linking-issn           = {1544-9173},
  location-id            = {10.1371/journal.pbio.2006601 [doi]},
  nlm-unique-id          = {101183755},
  owner                  = {NLM},
  publication-status     = {epublish},
  registry-number        = {0 (Antibodies, Viral)},
  revised                = {20190619},
