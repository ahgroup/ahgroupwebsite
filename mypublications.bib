  owner                  = {NLM},
  print-issn             = {1539-3755},
  publication-status     = {ppublish},
  revised                = {20061009},
  source                 = {Phys Rev E Stat Nonlin Soft Matter Phys. 2006 Sep;74(3 Pt 2):036302. doi: 10.1103/PhysRevE.74.036302. Epub 2006 Sep 14.},
  status                 = {PubMed-not-MEDLINE},
  title-abbreviation     = {Phys Rev E Stat Nonlin Soft Matter Phys},
}

@Article{handel05,
